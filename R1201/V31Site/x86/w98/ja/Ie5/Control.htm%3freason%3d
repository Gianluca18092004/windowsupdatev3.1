<SCRIPT Language="VBScript">
On Error Resume Next
If top.g_bIsWinUpdate <> TRUE Then
	top.location.replace("../../../../default.htm")
End if
 Top.Frames("frmToolbar").HideWaitDialog()
On Error Goto 0
</SCRIPT>
<SCRIPT Language="VBScript">
Dim g_strReason
If location.search <> "" Then
 Dim strStart, strEnd
 strStart = Instr(LCase(location.search),"reason=") + 7
 strEnd = Instr(strStart, location.search, "&")
 if strEnd = 0 Then strEnd = Len(location.search) + 1
	g_strReason = Mid(location.search, strStart, strEnd - strStart)
End If
Sub TryAgain
	Parent.GblSync = "ProductUpdates"
	parent.frames("FrmToolbar").document.frames(0).location.replace "engine.htm"
End Sub
Sub Window_Onload
 Select Case LCase(g_strReason)
 Case "unsupportedlang"
 dvExplanation.innerHTML = dvExplanation.innerHTML & "<P>製品カタログを読み込めませんでした。オペレーティング システムで使用されている言語は、現在のところ Windows Update でサポートされていません。"
	Case "catalogerror"
 dvExplanation.innerHTML = dvExplanation.innerHTML & "<P>製品カタログを読み込み中にエラーが発生しました。ネットワーク エラーが発生したか、またはファイルが壊れている可能性があります。詳しい情報は Microsoft サポート技術情報 (KB) の <a href=http://support.microsoft.com/support/kb/articles/q243/7/87.asp target=_top>Q243787</a> を参照してください。</ID><BR><BR>"
 End Select
End Sub
</SCRIPT>
